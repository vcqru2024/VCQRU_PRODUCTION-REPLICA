            function getQueryStrings() {
           
                var decode = function (s) { return decodeURIComponent(s.replace(/\+/g, " ")); };
                var queryString = location.search.substring(1);
                var keyValues = queryString.split('&');
                for (var i in keyValues) {
                    var key = keyValues[i].split('=');
                    rquestpage = key[0];
                    if (rquestpage == "mlt") {
                        $.ajax({
                            type: "post",
                            url: '<%=ProjectSession.absoluteSiteBrowseUrl%>/Info/MasterHandler.ashx?method=verifyresetpwd&mlt=' + key[1],
                            success: function (data) {
                                var result = data;
                                if (result == "yes") {
                                    $.confirm({
                                        title: 'Reset Password',
                                        content: 'url:resetpassword.html',
                                        onContentReady: function () {
                                            this.$content.find('label#mltid').text(key[1]);
                                        },
                                        boxWidth: '800px',
                                        useBootstrap: false,
                                        buttons: {
                                            sayMyName: {
                                                text: 'Confirm',
                                                btnClass: 'btn-warning',
                                                action: function () {
                                                    
                                                    var input = this.$content.find('input#newpwd').val();
                                                    var cnfinput = this.$content.find('input#confirmpwd').val();
                                                    var errorText = this.$content.find('.text-danger');
                                                    if (input == '') {
                                                        errorText.html('<div class="text-danger help-block">Please enter new password<div>').slideDown(200);
                                                        return false;
                                                    } else if (cnfinput == '') {
                                                        errorText.html('<div class="text-danger help-block">Please enter confirm password<div>').slideDown(200);
                                                        return false;
                                                    }
                                                    if (input == cnfinput) {
                                                      
                                                        $.ajax({
                                                            type: "post",
                                                            url: '<%=ProjectSession.absoluteSiteBrowseUrl%>/Info/MasterHandler.ashx?method=setpassword&pwd=' + cnfinput + '&value=' + key[1],
                                                            success: function (data) {
                                                                var result = data;
                                                                toastr.info(result);
                                                            },
                                                        });
                                                    }
                                                    else {
                                                        errorText.html('<div class="text-danger help-block">Password not match<div>').slideDown(200);
                                                        return false;
                                                    }
                                                }
                                            },
                                            cancel: function () {
                                                $.alert('Canceled!');
                                            }
                                        }
                                    });
                                }
                                else {
                                    toastr.info(result);
                                }
                            },
                        });
                    }
                    if (rquestpage == "key") {
                        $('#progress').show();
                        $.ajax({
                            type: "post",
                            url: '<%=ProjectSession.absoluteSiteBrowseUrl%>/Info/MasterHandler.ashx?method=verifyaccount&key=' + key[1],
                            success: function (data) {
                                var result = data;
                                toastr.info(result);
                            },
                        });
                        $('#progress').hide();
                    }
                    if (rquestpage == "Del") {
                        $.ajax({
                            type: "post",
                            url: '<%=ProjectSession.absoluteSiteBrowseUrl%>/Info/MasterHandler.ashx?method=deleteaccount&Del',
                            success: function (data) {
