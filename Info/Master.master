                    }
                    if (rquestpage == "key") {
                        $('#progress').show();
                        $.ajax({
                            type: "post",
                            url: '<%=ProjectSession.absoluteSiteBrowseUrl%>/Info/MasterHandler.ashx?method=verifyaccount&key=' + key[1],
                            success: function (data) {
                                var result = data;
                                toastr.info(result);
                            },
